# compton
compton -Ccf &

# set the gamma
xgamma -gamma 0.85 &

# fonts
fonts.sh &

# touchpad
touchpad.sh &

# beep off
xset -b &

# urxvt daemon
urxvtd &

# set the background
./.fehbg &

# bar
sleep 2s && bar.sh &

# start mpd
mpd &

# clipmenu
clipmenud &

# welcome message
sleep 4s && notify-send "welcome home" &

# mpd scrobbler
sleep 1m && mpdas -c /home/eti/.mpdasrc &