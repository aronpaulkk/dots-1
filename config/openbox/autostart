# compton
compton -Ccf &

# set the gamma
xgamma -gamma 0.8 &

# fonts
/usr/scripts/fonts.sh &

# beep off
xset -b &

# urxvt daemon
urxvtd &

# set the background
./.fehbg &

# bar
sleep 1s && /usr/scripts/bar.sh &

# start mpd
mpd &

# clipmenu
clipmenud &

# mpd scrobbler
sleep 1m && mpdas -c /home/eti/.mpdasrc &